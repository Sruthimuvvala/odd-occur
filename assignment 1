def odd_occurence(my_list, list_size):

   for i in range(0, list_size):
      count = 0
      for j in range(0, list_size):
         if my_list[i] == my_list[j]:
            count+= 1

      if (count % 2 != 0):
         return my_list[i]

   return -1
my_list = [ 1,2,3,2,1,3,2,1,2,2,2,2 ]
print(my_list)
n = len(my_list)
print("The elements :")
print(odd_occurence(my_list, n))
